@model IEnumerable<Morgenmadsbuffeten_GruppeTo.Models.Entity.Guest>
@{
    ViewData["Title"] = "Home";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div style="align-content: center">
    <h1>Welcome to the breakfast buffet system</h1>
    <img src="~/images/fontpageimg.png" class="ImgFontpage" />
    <p style="margin-left:2%">Please login to continue. If you are a first time user please create an account by clicking register</p>
    <a asp-area="Identity" asp-page="/Account/Login" class="IndexBtnLogin">Login</a>
    <a asp-area="Identity" asp-page="/Account/Register" class="IndexBtnRegister">Register</a>
</div>
