<Application x:Class="DebtBook.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:DebtBook.ViewModels"
             StartupUri="Views/MainWindow.xaml">
    <Application.Resources>
        <local:ViewModelLocator x:Key="ViewModelLocator"/>
        <SolidColorBrush x:Key="Brush" Color="LightBlue"/>
        <SolidColorBrush x:Key="BrushDepter" Color="LightGoldenrodYellow"/>
        <SolidColorBrush x:Key="BrushAdd" Color="LightGreen"/>
    </Application.Resources>
</Application>
